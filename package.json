{
  "name": "haircare-ingredients-analyzer",
  "version": "1.0.0",
  "description": "Library for analyzing haircare product ingredients",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --verbose",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky",
    "type-check": "tsc --noEmit",
    "trace": "tsc --traceResolution > temp/trace.log",
    "api-extractor": "npm run build && api-extractor run --local --diagnostics --config api-extractor.json",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write 'src/**/*.{ts,tsx}'"
  },
  "keywords": [
    "haircare",
    "ingredients",
    "analysis"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@microsoft/api-extractor": "^7.48.0",
    "@microsoft/tsdoc": "^0.15.1",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "fuse.js": "^6.6.2",
    "uuid": "^11.0.3"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "lint-staged": {
    "*.ts": [
      "eslint --fix"
    ]
  }
}
